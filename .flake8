[flake8]
ignore =
    D203,
    E126,
max-line-length = 120
exclude =
    _version.py,
	_*_version.py,
    _versioneer.py,
    versioneer.py,
    .svn,
    CVS,
    .bzr,
    .hg,
    .git,
    __pycache__,
    .tox,
    __init__.py,
    dummy_miz.py,
    build,
    dist,
    output,
    .cache,
    .hypothesis,
    qt_resource.py,
    _parking_spots.py,
    ./test/*,
    ./.eggs/*,
max-complexity = 10